{
  "rules": {
    "block-scoped-var": 1,
    "brace-style": [1, "1tbs"],
    "camelcase": 1,
    "comma-dangle": [1, "always-multiline"],
    "consistent-return": 1,
    "curly": 1,
    "eol-last": 1,
    "eqeqeq": 2,
    "no-nested-ternary": 1,
    "padded-blocks": 0,
    "quotes": 0,
    "no-cond-assign": 2,
    "no-constant-condition": 2,
    "no-duplicate-case": 2,
    "no-dupe-keys": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-undef": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "semi": [2, "always"],
    "space-before-function-paren": [1, "never"],
    "space-before-blocks": [1, "always"],
    "strict": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "angular/angularelement": 1,
    "angular/controller-as": 2,
    "angular/controller-as-route": 2,
    "angular/controller-as-vm": [2, "vm"],
    "angular/controller-name": [2, "/[A-Z].*Ctrl$/"],
    "angular/deferred": 0,
    "angular/definedundefined": 2,
    "angular/di": [2, "function"],
    "angular/di-order": [0, true],
    "angular/directive-name": [1,"ps|ui"],
    "angular/directive-restrict": [0, {
      "restrict": "AE",
      "explicit": "never"
    }],
    "angular/component-limit": [0, 1],
    "angular/document-service": 2,
    "angular/empty-controller": 0,
    "angular/file-name": 0,
    "angular/filter-name": 0,
    "angular/foreach": 0,
    "angular/function-type": 0,
    "angular/interval-service": 2,
    "angular/json-functions": 2,
    "angular/log": 2,
    "angular/module-dependency-order": [0, {
      "grouped": true,
      "prefix": null
    }],
    "angular/module-getter": 2,
    "angular/module-name": 0,
    "angular/module-setter": 2,
    "angular/no-angular-mock": 0,
    "angular/no-controller": 0,
    "angular/no-cookiestore": 2,
    "angular/no-http-callback": 2,
    "angular/no-inline-template": [0, {
      "allowSimple": true
    }],
    "angular/no-jquery-angularelement": 2,
    "angular/no-private-call": 2,
    "angular/no-run-logic": [0, {
      "allowParams": true
    }],
    "angular/no-service-method": 0,
    "angular/no-services": [2, ["$http", "$resource", "$q"]],
    "angular/on-watch": 2,
    "angular/one-dependency-per-line": 0,
    "angular/rest-service": 0,
    "angular/service-name": 2,
    "angular/timeout-service": 2,
    "angular/typecheck-array": 2,
    "angular/typecheck-date": 2,
    "angular/typecheck-function": 2,
    "angular/typecheck-number": 2,
    "angular/typecheck-object": 2,
    "angular/typecheck-string": 2,
    "angular/watchers-execution": [0, "$digest"],
    "angular/window-service": 2
  },
  "plugins": [
    "angular", "html"
  ],
  "globals": {
    "chrome": false,
    "angular": false
  }
}
